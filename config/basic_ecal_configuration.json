        "bins": 50,
        "title": "$R_{9}$"
    },
    "e5x5SCEle": {
        "range" : [0, 500],
        "bins"  :  50,
        "title" : "$E_{5\\times 5}$",
        "adjust": true
    },
    "nPV": {
        "range": [0, 50],
        "bins": 50,
        "title": "$N_{pv}$",
        "adjust": true
    },
    "rho": {
        "range" : [0, 50],
